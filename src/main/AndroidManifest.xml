            android:theme="@style/Theme.Main"
            android:screenOrientation="portrait"
            android:parentActivityName="com.patron.main.FlashMenu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.patron.main.FlashMenu" />
